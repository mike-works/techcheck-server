{
  "name": "aws-nodejs-typescript",
  "version": "0.0.1",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "repository": "https://github.com/mike-works/techcheck-server",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "scripts": {
    "start": "./node_modules/.bin/serverless offline start",
    "deploy": "./node_modules/.bin/serverless deploy",
    "test": "./node_modules/.bin/mocha -o test/mocha.opts",
    "test:watch": "./node_modules/.bin/mocha-typescript-watch",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@mike-north/js-lib-renovate-config": "1.0.0",
    "@mike-north/js-lib-semantic-release-config": "1.0.0",
    "@types/aws-lambda": "8.10.15",
    "@types/aws-sdk": "2.7.0",
    "@types/chai": "4.1.7",
    "@types/mocha": "2.2.48",
    "@types/node": "9.6.36",
    "chai": "4.2.0",
    "husky": "1.1.3",
    "mocha": "4.1.0",
    "mocha-typescript": "1.1.17",
    "semantic-release": "15.10.8",
    "serverless": "1.32.0",
    "serverless-offline": "3.31.2",
    "serverless-webpack": "4.4.0",
    "travis-deploy-once": "5.0.9",
    "ts-loader": "3.5.0",
    "ts-node": "7.0.1",
    "typescript": "2.9.2",
    "webpack": "3.12.0"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.181.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
